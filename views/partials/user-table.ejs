<form action="/user-change">
    <table class="manage-users">
        <tr>
            <th>id</th><th>name</th><th>password</th><th>amount</th>
        </tr>
        <% all.forEach(function(item) { %>
            <tr>
                <td><input type="text" value="<%= item.id %>" name="id" readonly></td>
                <td><input type="text" value="<%= item.name %>" name="name"></td>
                <td><input type="text" value="<%= item.pass %>" name="password"></td>
                <td><input type="text" value="<%= item.amount %>" name="amount"></td>
            </tr>
        <% }) %>
    </table>
    <input type="submit" value="Make change" name="user-change">
</form>
