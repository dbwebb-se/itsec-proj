<form class="manage-part" action="/manage/account/update">
    <table class="manage-users">
        <h1>Account information</h1>
        <tr>
            <th>id</th><th>Accountname</th><th>Amount</th>
        </tr>
        <% session.accounts.forEach(function(item) { %>
            <tr>
                <td><input type="text" value="<%= item.id %>" name="acc_id" readonly></td>
                <td><input type="text" value="<%= item.acc_name %>" name="acc_name"></td>
                <td><input type="text" value="<%= item.amount %>" name="acc_amount" readonly></td>
            </tr>
        <% }) %>
        <tr><td><input type="submit" value="Change accountname" name="change"></td></tr>
    </table>
</form>
